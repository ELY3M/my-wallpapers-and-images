function modelThumbnailMouseover(src){
ids = src.id.split("_");
size_id = ids[1];
model_id = ids[2];
if(allModelsMedium.length > 0){
displayurl = allModelsMedium[model_id][size_id];
mediumimage = document.getElementById('model_medium_image');
mediumimage.src = displayurl;
doIterate = false;
}
}
function modelThumbnailMouseout(src){
doIterate = true;
}
